alias 42check='bash ~/42FileChecker/42FileChecker.sh'
alias clean='rm -f *.o ; rm -f a.out ; rm -f .*.swp ; rm -f .*.swo ; rm *.gch'
alias majlib='rm -f libft/* && cp /Users/awajsbro/project/libft/* libft/ && rm libft/auteur'
alias remakelib='make re /Users/awajsbro/project/libft && make clean /Users/awajsbro/project/libft'









